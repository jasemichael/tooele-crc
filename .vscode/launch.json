    "version": "0.2.0",
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Run ios",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${file}",
            "runtimeExecutable": "npm",
            "runtimeArgs": ["run", "ios"]
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Run android",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${file}",
            "runtimeExecutable": "npm",
            "runtimeArgs": ["run", "android"]
        }
    ]
}